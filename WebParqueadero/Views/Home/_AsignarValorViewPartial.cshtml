@model WebParqueadero.Models.Documento

<div class="form-group">
    <div class="col-lg-12">
        TIEMPO TRANSCURRIDO
    </div>
    <div class="input-group col-lg-12">
        <div class="input-group-prepend">
            <span class="input-group-text" id="inputGroupPrepend">
                <i class="fas fa-hourglass-end"></i>
            </span>
            @Html.DisplayFor(m => Model.DetalleDocumento.FirstOrDefault().Transcurrido_DDoc, new { @class = "text-left" })
        </div>
    </div>
</div>
<div class="form-group">
    <div class="col-lg-12">
        VALOR
    </div>
    <div class="input-group col-lg-12">
        <div class="input-group-prepend">
            <span class="input-group-text" id="inputGroupPrepend">
                <i class="fas fa-dollar-sign"></i>
            </span>
            @Html.EditorFor(model => model.Valor_Doc, new { htmlAttributes = new { @class = "form-control col-sm-12", placeholder = "Ingrese placa", title = "Ingrese placa" } })
            @*@Html.DisplayFor(m => Model.Valor_Doc, new { @class = "text-left" })*@
        </div>
    </div>
</div>

